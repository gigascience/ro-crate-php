{
    "name": "alex/my-php-app",
    "description": "attempt",
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Json\\": "src/Json",
            "Exceptions\\": "src/exceptions",
            "ROCrate\\": "src/rocrate"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "monolog/monolog": "^3.0"
    },
    "config": {
        "vendor-dir": "vendor",
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
